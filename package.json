{
  "name": "react-native-notification-manager-lokal",
  "version": "1.0.0",
  "description": "Complete React Native port of Android notification functionality with exact same logic for ordering, re-pushing to top, and device-specific handling",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "scripts": {
    "build": "bob build",
    "prepare": "bob build",
    "test": "jest",
    "lint": "eslint src",
    "typescript": "tsc --noEmit",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "notifications",
    "android",
    "push-notifications",
    "notification-management",
    "notification-ordering",
    "notification-refresh",
    "deep-linking",
    "analytics"
  ],
  "author": "Lokal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lokal-app/react-native-notification-manager-lokal.git"
  },
  "bugs": {
    "url": "https://github.com/lokal-app/react-native-notification-manager-lokal/issues"
  },
  "homepage": "https://github.com/lokal-app/react-native-notification-manager-lokal#readme",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "@types/node": "^20.0.0",
    "react-native-builder-bob": "^0.29.0",
    "typescript": "^5.0.0",
    "eslint": "^9.0.0",
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint-plugin-jest": "^28.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "release-it": "^17.0.0"
  },
  "files": [
    "lib/",
    "android/",
    "ios/",
    "README.md",
    "NATIVE_IMPLEMENTATION_GUIDE.md",
    "CHANGELOG.md"
  ],
  "react-native": "src/index",
  "codegenConfig": {
    "name": "RNNotificationManagerSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}